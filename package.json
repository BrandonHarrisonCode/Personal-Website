{
  "name": "brandon-harrison-portfolio",
  "version": "1.0",
  "description": "Brandon Harrison's portfolio and resume.",
  "private": true,
  "scripts": {
    "test": "prettier --check 'src/**/*.css' 'src/**/*.js' 'src/**/*.html' && htmlhint src/**/*.html && stylelint src/**/*.css && eslint src/**/*.js",
    "format": "prettier --write 'src/**/*.css' 'src/**/*.js' 'src/**/*.html'",
    "build": "webpack --progress --config webpack.prod.js",
    "start": "webpack serve --open --config webpack.dev.js"
  },
  "author": "Brandon Harrison",
  "devDependencies": {
    "css-loader": "^5.2.5",
    "css-minimizer-webpack-plugin": "^3.0.0",
    "eslint": "^7.27.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "extract-loader": "^5.1.0",
    "html-loader": "^2.1.2",
    "html-webpack-plugin": "^5.3.1",
    "htmlhint": "^0.14.2",
    "mini-css-extract-plugin": "^1.6.0",
    "prettier": "2.3.0",
    "style-loader": "^2.0.0",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-order": "^4.1.0",
    "stylelint-prettier": "^1.2.0",
    "webpack": "^5.37.1",
    "webpack-cli": "^4.7.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3"
  }
}
